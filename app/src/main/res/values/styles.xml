<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorGreenVelvet</item>
        <item name="colorPrimaryDark">@color/colorGreenPlusSombre</item>
        <item name="colorAccent">@color/jone</item>
        <item name="android:navigationBarColor">@color/black</item>
    </style>

    <style id="mybutton" name="mybuttonstyle" parent="@android:style/Widget.Button">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">16dp</item>
        <item name="android:textStyle">bold</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="text">
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <style name="textpara">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">25dp</item>
        <item name="fontFamily">casual</item>
        <item name="android:gravity">center</item>

    </style>

    <style name="AlertDialogTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:color">@color/black</item>
    </style>
        an
</resources>
